{
    "name": "elephantpink/blog",
    "description": "A simple blog package for Laravel 6.",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel RamÃ­rez",
            "email": "dramirez@elephantpink.es"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "EPink\\Blog\\BlogServiceProvider"
            ]
        },
        "npm": {
            "laravel-mix-svg-vue": "^0.2.6",
            "sass": "^1.20.1",
            "sass-loader": "7.*",
            "vue": "^2.5.17",
            "vue-template-compiler": "^2.6.10",
            "vue-cropperjs": "^4.0.1",
            "vue-quill-editor": "^3.0.6",
            "vue-router": "^3.1.4",
            "vuex": "^3.1.2"
        }
    },
    "autoload": {
        "classmap": [
            "database/migrations",
            "database/factories"
        ],
        "psr-4": {
            "EPink\\Blog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.0",
        "laravel/framework": ">=6.0",
        "laravel/passport": "^8.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "post-install-cmd": [
        "npm install --save cropperjs lang.js svg-vue vue vue-cropperjs vue-quill-editor vue-router vuex"
    ]
}
